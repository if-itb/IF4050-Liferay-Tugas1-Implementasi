<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.social">
	<author>alifradityar</author>
	<namespace>socialservice</namespace>

	<entity name="Message" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="messageId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="title" type="String" />
		<column name="content" type="String" />
		<column name="date" type="Date" />
		
	</entity>
</service-builder>