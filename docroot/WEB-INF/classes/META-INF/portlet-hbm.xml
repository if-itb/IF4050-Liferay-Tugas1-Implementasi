<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.social.model.Message" />
	<class name="com.social.model.impl.MessageImpl" table="socialservice_Message">
		<cache usage="read-write" />
		<id name="messageId" type="long">
			<generator class="assigned" />
		</id>
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="content" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="date" type="org.hibernate.type.TimestampType" column="date_" />
	</class>
</hibernate-mapping>